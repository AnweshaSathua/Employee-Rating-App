{
  "version": 2,
  "builds": [
    {
      "src": "dist/employee-rating-app/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/employee-rating-app/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/.*", "dest": "/server/server.mjs" }
  ]
}
